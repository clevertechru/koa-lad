  "lint-staged": {
    "linters": {
      "*.{js,jsx,mjs,ts,tsx,css,less,scss,json,graphql}": [
        "prettier --write",
        "git add"
      ],
      "*.md": [
        "git add"
      ],
      "*.md,!test/snapshots/**/*.md,!test/**/snapshots/**/*.md,!locales/README.md": [
        "remark . -qfo"
      ]
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/lad"
  },
  "scripts": {
    "start": "nps",
    "build": "nps build",
    "watch": "nps watch",
    "test": "nps test",
    "test-coverage": "nps test-coverage",
    "lint": "nps lint",
    "coverage": "nps coverage",
    "precommit": "nps precommit",
    "pretest": "nps pretest"
  },
  "xo": {
    "extends": "prettier",
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "trailingComma": "none"
        }
      ],
      "max-len": [
        "error",
        {
          "code": 100,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off",
      "camelcase": "off",
      "no-warning-comments": "off"
    },
    "space": true,
    "overrides": [
      {
        "plugins": [
          "prettier"
        ],
        "files": [
          "test/*.js",
          "test/**/*.js"
        ],
        "globals": [
          "web",
          "api"
        ]
      },
      {
        "plugins": [
          "prettier",
          "compat"
        ],
        "settings": {
          "polyfills": [
            "fetch"
          ]
        },
        "parserOptions": {
          "sourceType": "script"
        },
        "files": [
          "assets/js/*.js",
          "assets/js/**/*.js"
        ],
        "envs": [
          "browser"
        ],
        "rules": {
          "compat/compat": "error"
        }
      }
    ]
  }
}
